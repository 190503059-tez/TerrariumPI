#: views/system_environment.tpl:280
msgid "Timer"
msgstr "Timer"

#: terrariumTranslations.py:79 terrariumTranslations.py:86
#: terrariumTranslations.py:104 terrariumTranslations.py:117
#: views/dashboard.tpl:83 views/dashboard.tpl:112 views/dashboard.tpl:159
#: views/dashboard.tpl:206 views/inc/usage_environment.tpl:34
#: views/inc/usage_environment.tpl:230 views/inc/usage_environment.tpl:324
